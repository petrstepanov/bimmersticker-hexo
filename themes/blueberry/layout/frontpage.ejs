<section class="pt-3 bg-dark-subtle spacer-footer">
	<%- partial('partial/heading', {title: 'Contact us', subtitle: 'Got sticker ideas or any questions? Use form below to contact us. We will get back to you as soon as possible!', name: 'contact'}) %>

	<div class="container-md justify-content-center mb-4">
		<div class="row justify-content-center">
			<div class="col-12 col-sm-10 col-md-8">
				<!-- Netlify Forms -->
				<form name="contact" data-netlify="true" class="card card-blueberry bg-body js--init-ajax-submit" method="POST" data-success-notification="Thanks for reaching out! We received your message and will email you back soon.">
					<div class="card-body">
						<label class="label-blueberry" for="input-email">Your email</label>
						<input type="email" required autocomplete="on" name="email" class="form-control" id="input-email">
						<small class="form-text text-body-secondary">We'll never share your email with anyone else.</small>
						<label class="label-blueberry mt-3" for="input-message">Your message</label>
						<textarea name="message" required class="form-control js--init-autosize" rows="4" id="input-message"></textarea>
						<label class="label-blueberry mt-3" for="input-message">Attach image</label>
						<input name="attachment" class="form-control-file" type="file" />
					</div>
					<div class="card-body text-end pt-0">
						<button type="button" class="btn btn-blueberry btn-outline-primary" data-bs-toggle="modal" data-bs-target="#contactModal">Contact info</button>
						<button type="submit" class="btn btn-blueberry btn-primary ms-2">Send message</button>
					</div>
				</form>
			</div>
		</div>
	</div>

	<% if (theme.googlereview_url){ %>
		<%- partial('partial/heading', {title: 'Leave us a review', name: 'review'}) %>
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-12 col-md-8 text-center">
					<p>Our business is leagally registered as Sticker Store LLC. Find our head office <a href="<%- theme.googlemaps_url %>" title="Sticker Store LLC on Google Maps">location on Google Maps</a>. Your <strong>honest review</strong> will help our business grow and provide better service for customers.</p>
						<a href="<%- theme.googlereview_url %>" title="Leave us a review on Google" class="btn btn-blueberry btn-primary mt-2">Leave us a review on Google</a>
				</div>
			</div>
		</div>
	<% } %>
</section>

<%- // partial('partial/mailchimp-dialog') %>
<%- partial('partial/contact-dialog') %>