<%
  // OpenGraph Image Settings - 1200x630 minimum
  page.photos = [config.url + '/img/sticker-store-llc-bimmer-sticker-store.jpg'];
%>

<section class="mb-3">
	<%- partial('partial/navbar') %>
	<div class="widget-jumbotron-frontpage bg-dark-subtle">
		<div class="container">
			<div class="row">
				<h1 class="col-11 col-md-12 col-lg-9 col-xl-8 display-4 mt-4 mt-lg-5 mb-5 mb-md-0"><%- theme.slogan ? theme.slogan : "" %></h1>
			</div>
			<div class="row">
				<div class="col-12 col-md-7 col-xl-5 mb-3   mt-5 mt-md-2">
					<p class="fs-4 my-3"><%- theme.lead1 ? theme.lead1 : "" %></p>
					<p class="fs-6"><%- theme.lead2 ? theme.lead2 : "" %></p>
				</div>
			</div>
			<div class="row">
				<div class="col mb-4 mb-lg-5">
					<div class="d-flex align-items-center">
						<a href="#" class="btn btn-lg btn-primary btn-blueberry me-2" data-name="info" data-domain="stickerstore" data-com="llc" onclick="window.location.href = 'mailto:' + this.dataset.name + '@' + this.dataset.domain + '.' + this.dataset.com; return false;">
							Send us an Email
						</a>
						<span class="d-none d-sm-inline front-paragraph">for custom orders.</span>
					</div>
				</div>
			</div>
		</div>
		<div class="wave-header"><%- partial('svg/wave-header.svg') %></div>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-12 col-sm-6 col-xxl-5 ms-xxl-auto me-xxl-auto">
				<div class="icon-xxl icon-padded blue mt-sm-1 mb-3 mb-lg-3"><%- partial('svg/icon-roll.svg') %></div>
				<p class="mb-4">We make stickers from the award-winning ORACAL® 651 and Cricut® adhesive outdoor vinyls. Contour cut vinyl stickers never fade out. <strong>Durability period is more than five years</strong>.</p>
			</div>
			<div class="col-12 col-sm-6 col-xxl-5 ms-xxl-auto me-xxl-auto">
				<div class="icon-xxl icon-padded blue mt-sm-1 mb-3 mb-lg-3"><%- partial('svg/icon-ship.svg') %></div>
				<p class="mb-4">
					<strong>Affordable shipping options via USPS</strong>. One day handling time. Guaranteed three to five days delivery time for US orders. We ship worldwide and offer flexible international shipping rates.
				</p>
			</div>
		</div>
	</div>
</section>
<section class="pb-2">
	<!-- Windshield banners -->
	<%- partial('partial/heading', {title: 'Windshield Banners and Sun Strips', subtitle: 'We offer a collection of <a href="/what-is-a-car-windshield-banner/">windshield banners</a> for project and show cars, stanced vehicles, jdm cars. <a href="/custom-windshield-banner-sun-strip/">Create your personal banner or sun strip</a> with our online tool.', name: 'banners'}) %>

	<div class="container">
		<div class="row mb-4">
			<div class="col-12">
				<figure class="card card-blueberry">
					<% carouselId = "carousel_windshield_banner"; %>
					<% var href = "/custom-windshield-banner-sun-strip/"; %>
					<div id="<%- carouselId %>" class="carousel" data-bs-ride="carousel">
						<div class="carousel-inner">
							<div class="carousel-item active">
								<a class="img-wrapper release-height-md" href="<%- href %>">
									<%- resp100('/custom-windshield-banner-sun-strip/','custom-text-windshield-banners-order-online', 3905, 1274, "Personalized windshield sun strips with custom text") %>
								</a>
							</div>
							<div class="carousel-item">
								<a class="img-wrapper release-height-md" href="<%- href %>">
									<%- resp100('/custom-windshield-banner-sun-strip/','any-wording-on-your-car-truck-windshield', 3905, 1274, "Custom windshield banners for your car or truck") %>
								</a>
							</div>
						</div>
						<button class="carousel-control-prev" type="button" data-bs-target="#<%- carouselId %>" data-bs-slide="prev">
							<span class="carousel-control-prev-icon" aria-hidden="true"></span>
							<span class="visually-hidden">Previous</span>
						</button>
						<button class="carousel-control-next" type="button" data-bs-target="#<%- carouselId %>" data-bs-slide="next">
							<span class="carousel-control-next-icon" aria-hidden="true"></span>
							<span class="visually-hidden">Next</span>
						</button>
					</div>
					<!-- Card body -->
					<div class="card-body text-center">
						<div class="row justify-content-md-center">
							<div class="col-12 col-lg-8">
								<h3 class="card-title font-size-21">Custom windshield banners and sun strips</h3>
								<p class="mb-2 pb-1">Use our online tool to create and order your personalized windshield banner and sun strip for car or truck. We offer a variety of fonts and colors. Same day processing and next day shipping on all orders.</p>
								<a href="/custom-windshield-banner-sun-strip/" class="btn btn-blueberry btn-primary">Design Your <span class="d-none d-sm-inline">Custom</span> Windshield Banner →</a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
	<%- partial('partial/category-posts-list-horizontal', {categoryName: 'Banners', order: 'desc'}) %>
</section>
<section class="pb-2">
	<!-- Keyrings -->
	<%- partial('partial/heading', {title: 'Personalized key rings with your vehicle information', subtitle: 'Organize your car fleet keys and always have your car VIN on you.', name: 'keychain'}) %>

	<div class="container">
		<div class="row">
			<div class="col-12">
				<figure class="card card-blueberry">
					<div class="row g-0 align-items-center">
						<div class="col-md-6">
							<a class="img-wrapper release-height-md" href="/buy-keyring-keychain-with-your-car-vin-number/">
								<img width="1680" height="1260" loading="lazy" class="w-100 h-auto" src="/buy-keyring-keychain-with-your-car-vin-number/large_buy-keychain-keyring-match-vehicle-color-with-vin.jpg" title="Personalized car key ring (key chain) with vehicle year, make, model and VIN number">
							</a>
						</div>
						<div class="col-md-6">
							<div class="card-body text-center">
								<p class="mb-2 pb-1">At Sticker Store LLC, we understand that cars are more than just machines. They are an extension of your identity and a source of pride. <a href="/custom-car-keychain-with-vin-number">Have your vehicle information</a> with you wherever you go!</p>
								<a href="/buy-keyring-keychain-with-your-car-vin-number/" class="btn btn-blueberry btn-primary">Create Your Key Ring<span> →</span></a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
</section>
<section class="pt-2 pt-mb-4 pt-lg-4 pb-2">
	<!-- Featured stickers -->
	<%- partial('partial/heading', {title: 'Stickers for Cars and Trucks', subtitle: 'Stickers come in a variety of colors. New designs are often in progress! Check our Stories on <a rel="me" href="https://www.instagram.com/bimmersticker.store/">Instagram</a> and subscribe for updates.', name: 'featured'}) %>
	<%- partial('partial/category-posts-list-horizontal', {categoryName: 'Featured', order: 'desc'}) %>
</section>
<section class="pb-4">
	<!-- Social stickers -->
	<%- partial('partial/heading', {title: 'Custom social network stickers', subtitle: 'Variety of stickers that boost your online social profiles. Buy a set for your personal Instagram account, YouTube channel or other social profile.', name: 'social'}) %>

	<div class="container">
		<div class="row">
			<div class="col-12">
				<figure class="card card-blueberry">
					<div class="row g-0 align-items-center">
						<div class="col-md-6">
							<a class="img-wrapper release-height-md" href="/custom-social-media-stickers/">
								<img width="1364" height="810" loading="lazy" class="w-100 h-auto" src="/custom-social-media-stickers/large_custom-social-media-stickers-on-car-window.jpg" title="Personalized social media stickers with your account name" alt="Custom social media stickers with your account name: Instagram, Facebook, Youtube, Snapchat, TikTok, SoundCloud">
							</a>
						</div>
						<div class="col-md-6">
							<div class="card-body text-center">
								<p class="mb-2 pb-1">Check out our <strong>custom social media stickers</strong> for Instagram, Facebook, YouTube, Snapchat, TikTok and SoundCloud social networks. Get more followers and help your business grow! </p>
								<a href="/custom-social-media-stickers/" class="btn btn-blueberry btn-primary">View Social Media Stickers →</a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
</section>
<section class="pb-4">
	<!-- Bimmers only -->
	<%- partial('partial/heading', {title: 'For Bimmers', subtitle: 'Stickers below are designed exclusively for BMW cars. Have a cool idea for a custom sticker? <a href="#contact">Contact us</a> and we will get back to you the same day.', name: 'bmw-special'}) %>
	<div class="container">
		<div class="row justify-content-center">
			<div class="col-12 col-md-8">
				<figure class="card card-blueberry">
						<a class="" href="/bmw-roundel-overlay-stickers/">
							<img width="2048" height="956" loading="lazy" class="w-100 h-auto" src="/bmw-roundel-overlay-stickers/large_emblem-overlay-stickers-for-bmw-cars.jpg" title="Personalize your BMW logo emblem with custom color or state flag" alt="Custom stickers for BMW emblems, roundels">
						</a>
						<div class="card-body text-center">
							<p class="mb-2 pb-1">We offer a collection of overlay stickers for BMW emblems. Customize your emblem color with virtually any color or design such as Maryland, Texas or American flag.</p>
							<a href="/bmw-roundel-overlay-stickers/" class="btn btn-blueberry btn-primary">View Roundel Stickers →</a>
						</div>
				</figure>
			</div>
		</div>
	</div>
	<%- partial('partial/category-posts-list-horizontal', {categoryName: 'Bmw Special', order: 'desc'}) %>
</section>
<section>
	<%- partial('partial/heading', {title: 'Stickers for BMW Generations', subtitle: 'We have designed a dedicated sticker for almost every BMW generation.', name: 'series-generations'}) %>
	<div class="container">
		<div class="row mb-4">
			<div class="col-12">
				<figure class="card card-blueberry">
					<a class="img-wrapper release-height-md" href="/stickers-for-bmw-generations/">
						<img width="1106" height="395" loading="lazy" class="w-100 h-auto" src="/stickers-for-bmw-generations/generations-banner.jpg" title="Vinyl stickers for BMW generations" alt="Vinyl stickers for BMW generations">
					</a>

					<!-- Card body -->
					<div class="card-body text-center">
						<div class="row justify-content-md-center">
							<div class="col-12 col-lg-8">
								<p class="mb-2 pb-1">We offer vinyl decals for all generations of BMW cars: from early iconic Shark models (e12, e28 etc..) to the modern BMW G series. Feel free to check them out!</p>
								<a href="/stickers-for-bmw-generations/" class="btn btn-blueberry btn-primary">View BMW Generations Stickers →</a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
</section>

