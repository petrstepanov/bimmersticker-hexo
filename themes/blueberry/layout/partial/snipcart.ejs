        <div hidden id="snipcart" data-api-key="<%- process.env.SNIPCART_API_KEY %>"  data-config-add-product-behavior=""></div>    
        <script async src="https://cdn.snipcart.com/themes/v3.0.22/default/snipcart.js"></script>
    <% } %>
<% } %>