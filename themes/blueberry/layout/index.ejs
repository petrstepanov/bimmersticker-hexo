<% page.photos = ['/img/bimmersticker-store-og-front-omage-bme-328i-windshield-banner-vinyl-sticker.jpg']; %>

<section class="section-intro">
	<div class="section-intro-mobile-bg">
		<%- // partial('partial/navbar', {className: 'transparent'}) %>
		<%- partial('partial/navbar') %>
		<div class="jumbotron jumbotron-fluid jumbotron-blueberry fix">
			<div class="container">
				<div class="row">
					<div class="col-11 col-sm-8 col-md-12 col-lg-4">
						<h1 class="display-4 mt-lg-3"><%- theme.slogan ? theme.slogan : "" %></h1>
					</div>
				</div>
				<div class="row">
					<div class="col col-md-8 col-lg-6 col-xl-5 mb-3">
						<p class="front-paragraph mt-4"><%- theme.lead1 ? theme.lead1 : "" %></p>
						<p class="front-paragraph"><%- theme.lead2 ? theme.lead2 : "" %></p>
					</div>
				</div>
				<div class="row">
					<div class="col mb-4 mb-md-5">
						<div class="d-flex align-items-center">
							<!-- <button type="button" class="btn btn-lg btn-primary rounded-pill mr-2" data-toggle="modal" data-target="#subscribeModal">
								Subscribe to newsletter
							</button>
							<span class="d-none d-sm-inline front-paragraph">and get new sticker alerts.</span> -->
							<a href="mailto:info@bimmersticker.store" class="btn btn-lg btn-primary rounded-pill mr-2">
								Send us an Email
							</a>
							<span class="d-none d-sm-inline front-paragraph">for custom orders.</span> -->
						</div>
					</div>
				</div>
			</div>
			<img class="w-100 h-auto d-lg-none" alt="jumbotron section shape" width="960" height="78" src="<%- partial('data-uri/wave-header') %>" />
		</div>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-12 col-sm-6 col-xl-5 col-xl-5">
				<div class="icon-xl mt-sm-1 mb-2 mb-lg-3"><%- partial('icons/icon-roll.svg') %></div>
				<p class="mb-4">We make stickers from the award-winning ORACAL® 651 and Cricut® adhesive outdoor vinyls. Contour cut vinyl stickers never fade out. <strong>Durability period is more than five years</strong>.</p>
			</div>
			<div class="col-12 col-sm-6 col-xl-5 offset-xl-1">
				<div class="icon-xl mt-sm-1 mb-2 mb-lg-3"><%- partial('icons/icon-ship.svg') %></div>
				<p>
					<strong>Affordable shipping options via USPS</strong>. One day handling time. Guaranteed three to five days delivery time for US orders.
				</p>
				<p class="mb-4 mb-lg-0">We ship worldwide and offer flexible international shipping rates starting at $1.20</p>
			</div>
		</div>
	</div>
</section>
<section class="pt-2 pt-mb-4 pt-lg-4 pb-4">
	<!-- Featured stickers -->
	<%- partial('partial/heading', {title: 'Stickers for cars, trucks and SUVs', subtitle: 'Stickers come in a variety of colors. New designs are often in progress! Check our Stories on <a rel="me" href="https://www.instagram.com/bimmersticker.store/">Instagram</a> and subscribe for updates.', name: 'featured'}) %>
	<%- partial('partial/category-posts-list', {categoryName: 'Featured', order: 'desc'}) %>
</section>
<section class="pb-4">
	<!-- Windshield banners -->
	<%- partial('partial/heading', {title: 'Windshield banners', subtitle: 'A collection of windshield banners for project and show cars, stanced vehicles, jdm cars. Check out our <a href="/custom-windshield-banner-sun-strip/">windshield banner builder</a> to create your personal banners or sun strip.', name: 'banners'}) %>

	<div class="container">
		<div class="row mb-4">
			<div class="col-12">
				<figure class="card card-blueberry">
					<% carouselId = "carousel_windshield_banner"; %>
					<% var url = "/custom-windshield-banner-sun-strip/"; %>
					<div id="<%- carouselId %>" class="carousel slide" data-ride="carousel">
						<div class="carousel-inner">
							<div class="carousel-item active">
								<a class="img-wrapper" href="<%- url %>">
									<img width="3905" height="1274" loading="lazy"  src="/custom-windshield-banner-sun-strip/custom-text-windshield-banners-order-online.jpg" class="d-block h-auto w-100" alt="Personalized windshield sun strips with custom text">
								</a>
							</div>
							<div class="carousel-item">
								<a class="img-wrapper" href="<%- url %>">
									<img width="3905" height="1274" loading="lazy"  src="/custom-windshield-banner-sun-strip/any-wording-on-your-car-truck-windshield.jpg" class="d-block h-auto w-100" alt="Custom windshield banners for your car or truck">
								</a>
							</div>
						</div>
						<a class="carousel-control-prev" href="#<%- carouselId %>" role="button" data-slide="prev">
						  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
						  <span class="sr-only">Previous</span>
						</a>
						<a class="carousel-control-next" href="#<%- carouselId %>" role="button" data-slide="next">
						  <span class="carousel-control-next-icon" aria-hidden="true"></span>
						  <span class="sr-only">Next</span>
						</a>
					</div>      					
					<!-- Card body -->
					<div class="card-body text-center">
						<div class="row justify-content-md-center">
							<div class="col-12 col-lg-8">
								<h3 class="card-title font-size-21">Custom windshield banners and sun strips</h3>
								<p class="mb-2 pb-1">Use our online tool to create and order your personal windshield banner and sun strip. We offer a variety of fonts and colors. Same day processing and next day shipping on all orders.</p>
								<a href="/custom-windshield-banner-sun-strip/" class="btn btn-blueberry btn-primary">Design your custom windshield banner<span class="d-none d-sm-inline"> →</span></a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
	<%- partial('partial/category-posts-list', {categoryName: 'Banners', order: 'desc'}) %>
</section>
<section class="pb-4">
	<!-- Social stickers -->
	<%- partial('partial/heading', {title: 'Custom social network stickers', subtitle: 'Variety of stickers that boost your online social profiles. Buy a set for your personal Instagram account, YouTube channel or other social profile.', name: 'social'}) %>

	<div class="container">
		<div class="row">
			<div class="col-12">
				<figure class="card card-blueberry">      
					<div class="row no-gutters align-items-center">
						<div class="col-md-6">
							<a class="img-wrapper" href="/custom-social-media-stickers/">
								<img width="1364" height="810" loading="lazy" class="w-100 h-auto" src="/custom-social-media-stickers/custom-social-media-stickers-on-car-window.jpg" title="Personalized social media stickers with your account name" alt="Custom social media stickers with your account name: Instagram, Facebook, Youtube, Snapchat, TikTok, SoundCloud">
							</a>
						</div>
						<div class="col-md-6">
							<div class="card-body text-center">
								<p class="mb-2 pb-1">Check out our <strong>custom social media stickers</strong> for Instagram, Facebook, YouTube, Snapchat, TikTok and SoundCloud social networks. Get more followers and help your business grow! </p>
								<a href="/custom-social-media-stickers/" class="btn btn-blueberry btn-primary">View Social Media Stickers<span class="d-none d-sm-inline"> →</span></a>
							</div>
						</div>
					</div>
				</figure>
			</div>
		</div>
	</div>
</section>
<section class="pb-4">
	<!-- Bimmers only -->
